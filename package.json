    },
    "devDependencies": {
        "@types/clone": "^2.1.0",
        "@types/dotenv": "^8.2.0",
        "@types/jest": "^26.0.19",
        "@types/node": "^14.14.16",
        "@types/uuid": "^8.3.0",
